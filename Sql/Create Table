
CREATE TABLE TB_CAS_DICOM_DATA
(
    DCM_ID	VARCHAR(18)	PRIMARY KEY,
    DCM_SET_ID	NUMBER(10),
    FILE_PTH	VARCHAR(50),
    DCM_DRT	VARCHAR(10),
    DICOM_DRT_ID	NUMBER(10),
    DS_TP	CHAR(1),
    FILE_SZ	NUMBER(20),
    PXL	NUMBER(10),
    FILE_WDTH	NUMBER(10),
    FILE_HIGH	NUMBER(10),
    LBD_TP	CHAR(1),
    FILE_ORGIN	VARCHAR(18),
    LOAD_DT	DATE,
    PRD_DT	DATE,
    DT_TXT	VARCHAR(18)
)
TABLESPACE TS_CAS_DICOM_DATA;

